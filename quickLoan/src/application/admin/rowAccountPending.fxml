<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="50.0" prefWidth="670.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.admin.RowAccountPending">
   <children>
      <Label fx:id="label_username" prefHeight="50.0" prefWidth="161.0" text="Congvt123">
         <font>
            <Font size="15.0" />
         </font>
         <padding>
            <Insets left="11.0" />
         </padding>
      </Label>
      <Label fx:id="label_email" prefHeight="50.0" prefWidth="274.0" text="nguyenthanhcongvt123@gmail.com">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <AnchorPane prefHeight="50.0" prefWidth="142.0">
         <children>
            <Button fx:id="button_detailpending" layoutX="30.0" layoutY="12.0" mnemonicParsing="false" onAction="#detailPending" text="Detail" />
         </children>
      </AnchorPane>
      <Button fx:id="button_reject" mnemonicParsing="false" onAction="#reject" prefHeight="53.0" prefWidth="50.0" style="-fx-background-color: transparent;">
         <graphic>
            <ImageView fitHeight="33.0" fitWidth="38.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../../Downloads/cross.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="button_accept" mnemonicParsing="false" onAction="#accept" prefHeight="50.0" prefWidth="50.0" style="-fx-background-color: transparent;">
         <graphic>
            <ImageView fitHeight="34.0" fitWidth="41.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../../Downloads/check.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button mnemonicParsing="false" prefHeight="28.0" prefWidth="61.0" style="-fx-background-color: transparent;" />
   </children>
</HBox>
